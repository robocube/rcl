            Assert.AreEqual ("#a_b,c,d", RCSymbolScalar.From ("a_b", "c", "d").ToString ());
            Assert.AreEqual ("#'1','2','3'", RCSymbolScalar.From ("1", "2", "3").ToString ());
            runner.Yield (closure, RCBlock.Empty);
          }
        }
      ?]
      :Test.TestProgrammaticSymbolBuilding {}
